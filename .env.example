# Discord Bot Token - Get from https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# OpenAI compatible API Base URL
ONYX_API_BASE=https://your-api-endpoint.example.com

# AI Model to use (e.g., claude-opus-4-1, gpt-4, etc.)
AI_MODEL=claude-opus-4-1

# Maximum concurrent AI requests (queue limit)
MAX_CONCURRENT_REQUESTS=3

# Allowed Channel ID (bot will only respond in this channel, leave empty for all channels)
ALLOWED_CHANNEL_ID=
